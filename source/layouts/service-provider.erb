    <div class="jumbotron">
      <h1><%= current_page.data.title %></h1>
    </div>
  </header>

  <section>
    <div class="container">
      <div class="row">
        <div class="col-md-9 section-content" id="content">
          <%= yield %>
        </div>
        <div class="col-md-9 section-content" id="service_provider_container">
      </div>
    </div>
  </section>

  <script src="javascripts/serviceProviders.js"></script>
<% end %>
