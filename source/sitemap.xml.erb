---
layout: false
directory_index: false
---
<% pages = sitemap.resources.reject{|r| r.is_a? Middleman::Sitemap::Extensions::RedirectResource }.find_all{|p| p.source_file.match(/\.html/) } %>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% pages.each do |p| %>
  <url>
    <loc><%= ENV['SITE_URL'] %>/<%=p.destination_path.gsub('/index.html','/')%></loc>
    <changefreq>monthly</changefreq>
  </url>
<% end %>
</urlset>
